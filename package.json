{
  "name": "youtube-client-scraper",
  "version": "1.0.0",
  "description": "A library to scrape important information off YouTube",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "prestart": "npm run compile",
    "start": "node build/index.js",
    "build": "tsup",
    "test": "ava",
    "prepare": "tsup",
    "prepublishOnly": "npm run lint:fix && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blhutchings/youtube-client-scraper.git"
  },
  "keywords": [
    "youtube-client-scraper",
    "youtube",
    "youtube video",
    "youtube channel",
    "youtube search",
    "scrape",
    "crawler"
  ],
  "author": "Ben Hutchings",
  "license": "ISC",
  "private": true,
  "bugs": {
    "url": "https://github.com/blhutchings/youtube-client-scraper/issues"
  },
  "homepage": "https://github.com/blhutchings/youtube-client-scraper#readme",
  "files": [
    "dist/**/*",
	"tsup.config.json"
  ],
  "dependencies": {
    "got": "^12.6.0",
    "http2-wrapper": "^2.2.0",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@types/got": "^9.6.12",
    "@types/node": "^18.15.0",
    "@types/tough-cookie": "^4.0.3",
    "ava": "^5.3.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "failWithoutAssertions": false
  }
}
