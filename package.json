  "homepage": "https://github.com/blhutchings/youtube-client-scraper#readme",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@types/got": "^9.6.12",
    "@types/tough-cookie": "^4.0.2",
    "got": "^12.6.0",
    "http2-wrapper": "^2.2.0",
    "tough-cookie": "^4.1.2",
    "tsup": "^7.2.0"
